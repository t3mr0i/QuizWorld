{
  "name": "quizaru",
  "version": "1.0.0",
  "description": "Interactive web-based quiz game with AI-generated questions",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "partykit:dev": "partykit dev",
    "partykit:deploy": "partykit deploy",
    "serve": "partykit dev",
    "kill-ports": "node scripts/kill-ports.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:client": "vitest run tests/quiz-game-client.test.js",
    "test:server": "vitest run tests/server.test.ts tests/validators.test.ts",
    "test:integration": "vitest run tests/integration.test.js",
    "test:performance": "vitest run tests/performance.test.js"
  },
  "keywords": [
    "quiz",
    "game",
    "multiplayer",
    "ai",
    "questions",
    "trivia"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "partykit": "^0.0.114",
    "socket.io": "^4.7.3"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.1.1",
    "nodemon": "^3.1.9",
    "vitest": "^3.1.1"
  }
}
